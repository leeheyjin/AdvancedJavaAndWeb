package kr.or.ddit.event.vo;

public class EventVO {
	private int ent_no   ;
	private String ent_type ;
	private String ent_date ;
	private String ent_start;
	private String ent_end  ;
	private String ent_file;
	private String ent_ttl;
	private String ent_dtl;
	
	public String getEnt_ttl() {
		return ent_ttl;
	}
	public void setEnt_ttl(String ent_ttl) {
		this.ent_ttl = ent_ttl;
	}
	public String getEnt_dtl() {
		return ent_dtl;
	}
	public void setEnt_dtl(String ent_dtl) {
		this.ent_dtl = ent_dtl;
	}
	public String getEnt_file() {
		return ent_file;
	}
	public void setEnt_file(String ent_file) {
		this.ent_file = ent_file;
	}
	public int getEnt_no() {
		return ent_no;
	}
	public void setEnt_no(int ent_no) {
		this.ent_no = ent_no;
	}
	public String getEnt_type() {
		return ent_type;
	}
	public void setEnt_type(String ent_type) {
		this.ent_type = ent_type;
	}
	public String getEnt_date() {
		return ent_date;
	}
	public void setEnt_date(String ent_date) {
		this.ent_date = ent_date;
	}
	public String getEnt_start() {
		return ent_start;
	}
	public void setEnt_start(String ent_start) {
		this.ent_start = ent_start;
	}
	public String getEnt_end() {
		return ent_end;
	}
	public void setEnt_end(String ent_end) {
		this.ent_end = ent_end;
	}
	
}
