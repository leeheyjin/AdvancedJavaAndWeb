package kr.or.ddit.campinfo.vo;

public class ChartCampVO {
	private String camp_name;
	private String camp_addr;
	private int amount;
	
	
	public String getCamp_name() {
		return camp_name;
	}
	public void setCamp_name(String camp_name) {
		this.camp_name = camp_name;
	}
	public String getCamp_addr() {
		return camp_addr;
	}
	public void setCamp_addr(String camp_addr) {
		this.camp_addr = camp_addr;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	
	
	@Override
	public String toString() {
		return "ChartCampVO [camp_name=" + camp_name + ", camp_addr=" + camp_addr + ", amount=" + amount + "]";
	}
}
